int add(int a, int b, int base) {
    int c;
    c = a + b;
    c = c + base;
    return c;
}

int mult_by_5(int x) {
    return x * 5;
}

int main(void) {
    int a;
    int b, c;
    a = 0;
    b = 5;
    if(a < 10) {
        a = a + 10;
        b = a + 10;
    }
    c = add(a, b, add(a, b, 0));
    c = mult_by_5(c);
}